{
  "name": "book-a-meal",
  "version": "1.0.0",
  "engines": {
    "node": "9.11.1",
    "npm": "5.7.1"
  },
  "description": "Book-A-Meal is an app designed to let you order meals from your favourite resturant at your convience. To use you can check a day's menu and place orders.",
  "main": "./server/app.js",
  "scripts": {
    "test": " cross-env NODE_ENV=test nyc mocha server/tests --recursive --exit",
    "pretest": "npm-run-all migration:test seed:test",
    "migration:test": "sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test",
    "seed:test": "sequelize db:seed:all --env test",
    "start": " node ./dist/server/app.js",
    "build": "npm-run-all build-mig build-model build-server",
    "build-server": "babel server -d dist/server",
    "build-model": "babel models -d dist/models",
    "build-mig": "babel migrations -d dist/migrations && babel config -d dist/config",
    "build-ui": "babel UI -d dist/UI",
    "serve": "node dist/app.js",
    "server": "node_modules/.bin/nodemon ./server/app.js --exec babel-node",
    "client": "npm run start-dev --prefix client",
    "start-dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint \"server/**/*.js\" --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rovilay/Book-A-Meal.git"
  },
  "author": "Akinola Ogooluwa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rovilay/Book-A-Meal/issues"
  },
  "homepage": "https://github.com/rovilay/Book-A-Meal#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "cors": "^2.8.4",
    "cross-env": "^5.1.5",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "http-proxy-middleware": "^0.18.0",
    "jsonwebtoken": "^8.2.1",
    "moment": "^2.22.1",
    "node-uuid": "^1.4.8",
    "npm-run-all": "^4.1.3",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "react-portal-tooltip": "^2.4.0",
    "sequelize": "^4.37.10",
    "sequelize-cli": "^4.0.0",
    "swagger-ui-express": "^3.0.9",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^4.1.6",
    "concurrently": "^3.5.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "istanbul": "^0.4.5",
    "istanbul-babel": "^1.0.1",
    "mocha": "^5.1.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.17.4",
    "nyc": "^11.7.1",
    "prettier": "^1.12.1",
    "sinon": "^5.0.7",
    "sinon-chai": "^3.0.0",
    "sinon-express-mock": "^2.0.0"
  }
}
