{
  "name": "book-a-meal",
  "version": "1.0.0",
  "engines": {
    "node": "9.11.1",
    "npm": "5.7.1"
  },
  "description": "Book-A-Meal is an app designed to let you order meals from your favourite resturant at your convience. To use you can check a day's menu and place orders.",
  "main": "./server/app.js",
  "scripts": {
    "test": " nyc --reporter=html --reporter=text mocha --compilers js:babel-core/register ./test --exit --timeout 5000ms",
    "start": " babel-node ./server/app.js",
    "build": "babel server -d dist",
    "serve": "node dist/app.js",
    "start-dev": "node_modules/.bin/nodemon ./server/app.js --exec babel-node NODE_ENV=production",
    "coverage": "nyc report --reporter=text-lcov --reporter=lcov | coveralls",
    "heroku-postbuild": "npm install && babel-node ./server/app.js NODE_ENV=production",
    "lint": "eslint \"server/**/*.js\" --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rovilay/Book-A-Meal.git"
  },
  "author": "Akinola Ogooluwa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rovilay/Book-A-Meal/issues"
  },
  "homepage": "https://github.com/rovilay/Book-A-Meal#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.2.1",
    "moment": "^2.22.1",
    "node-uuid": "^1.4.8",
    "nodemon": "^1.17.3",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.37.6",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "istanbul": "^0.4.5",
    "istanbul-babel": "^1.0.1",
    "mocha": "^5.1.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.7.1",
    "prettier": "^1.12.1"
  }
}
